{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "client/dist",
  "functions": {
    "server/routes.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/$1"
    }
  ]
}